<?xml version="1.0" encoding="utf-8" ?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>N2Bootstrap</id>
    <version>$version$</version>
    <authors>Paul Knopf</authors>
    <description>An Twitter Bootstrap implementation of N2CMS with widgets/components from TBS. This also serves as a base project for extensions (blog, podcast, etc).</description>
    <language>en-US</language>
    <projectUrl>http://github.com/theonlylawislove/N2Bootstrap</projectUrl>
    <licenseUrl>http://github.com/theonlylawislove/N2Bootstrap</licenseUrl>
    <dependencies>
      <dependency id="N2CMS" version="[2.4.11.1]" />
      <dependency id="N2Bootstrap.Library" version="[$(version)]" />
      <dependency id="N2CMS.Management" version="[2.4.11.1]" />
      <dependency id="N2CMS.Razor" version="[2.4.11.1]" />
      <dependency id="DotlessClientOnly" version="[1.3.1.0]" />
      <dependency id="Microsoft.AspNet.Mvc" version="[4.0.20710.0]" />
      <dependency id="Microsoft.AspNet.WebPages" version="[2.0.20710.0]" />
      <dependency id="Cassette.Aspnet" version="[2.0.0]" />
    </dependencies>
  </metadata>
  <files>
    <file src="readme.txt" target="readme.txt"/>
    <file src="N2Bootstrap.dll" target="lib\net40" />
    <file src="N2Bootstrap.pdb" target="lib\net40" />
    <file src="_PublishedWebsites\N2Bootstrap\Web.config.transform" target="content\web.config.transform" />
    <file src="_PublishedWebsites\N2Bootstrap\Bootstrap\Bootstrap.zip" target="content\Bootstrap\Bootstrap.zip" />
    <file src="_PublishedWebsites\N2Bootstrap\Bootstrap\notes.txt" target="content\Bootstrap\notes.txt" />
    <file src="_PublishedWebsites\N2Bootstrap\Bootstrap\Themes\notes.txt" target="content\Bootstrap\Themes\notes.txt" />
    <file src="_PublishedWebsites\N2Bootstrap\Bootstrap\web.config" target="content\Bootstrap\web.config" />
    <file src="_PublishedWebsites\N2Bootstrap\App_Data\n2_host.config.transform" target="content\App_Data\n2_host.config.transform" />
    <file src="_PublishedWebsites\N2Bootstrap\App_Data\SampleBootstrap.n2.xml.gz" target="content\App_Data\SampleBootstrap.n2.xml.gz" />
    <file src="_PublishedWebsites\N2Bootstrap\App_Data\SampleBootstrap.n2.xml.gz.txt" target="content\App_Data\SampleBootstrap.n2.xml.gz.txt" />
    <file src="_PublishedWebsites\N2Bootstrap\Upload\**\*.*" target="content\Upload\" />
  </files>
</package>